<%
	Object.keys(report.files).forEach(function (key) {

		if (!report.files[key].length) {

			print('PASS\t');
			print(key);
			print('\n');

		} else {

			print('FAIL\t');
			print(key);
			print(report.files[key].length ? ' (' + report.files[key].length + ')\n': '\n');

			report.files[key].forEach(function (lintError) {

				print((lintError.file + ':' + lintError.line));
				print((lintError.character ? ':' + lintError.character : '') + '\t');
				print(lintError.reason || 'Unused variable `' + lintError.name + '`');
				print('\n');

			});

		}

	});

	if (report.failures) {

		print(('\n# JSLint failed, ' + report.failures + ' violations in ' + report.files_in_violation + ' files.  ' + report.file_count + ' files scanned.'));

	}
%>